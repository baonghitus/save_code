{
  "name": "bao-react-app",
  "private": true,
  "main": "./main/index.js",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "tsc && vite build",
    "build:electron": "tsc -p electron",
    "preview": "vite preview",
    "prettier": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "lint": "eslint src --config .eslintrc.js --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --config .eslintrc.js --ext .js,.jsx,.ts,.tsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:snap": "vitest -u",
    "lint-staged": "yarn prettier && yarn lint && tsc --noEmit",
    "prepare": "husky install",
    "clean": "rimraf dist main src/out",
    "format": "yarn prettier && yarn lint:fix",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "electron-is-dev": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.39.1",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@tsconfig/vite-react": "^1.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "dotenv": "^16.0.1",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "happy-dom": "^4.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "msw": "^0.42.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react-desktop": "^0.3.9",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "uuidv4": "^6.2.13",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.13.0"
  },
  "build": {
    "appId": "baoapp",
    "productName": "Bao",
    "category": "bao.baoapp.app.dmg",
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip"
      ]
    },
    "win": {
      "publisherName": "Simon Inc.",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "tar.gz"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "vkiranmaniya",
        "repo": "accurate",
        "vPrefixedTagName": true,
        "private": true,
        "releaseType": "draft"
      }
    ],
    "dmg": {
      "icon": "build/icon.icns"
    },
    "directories": {
      "buildResources": "resources"
    }
  }
}
